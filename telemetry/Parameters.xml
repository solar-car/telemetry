<?xml version="1.0" encoding="utf-8" ?>
<Parameters>
    <Modules>
        <DHT11>
            <Temperature mode="Input" unit="C">4</Temperature>
            <Test mode="Input" unit=":)">1</Test>
        </DHT11>
        <Example_Accelerometer>
            <Velocity mode="Input" unit="m/s">4</Velocity>
            <Debug mode="Output" unit=":)">32</Debug>
        </Example_Accelerometer>
    </Modules>

    <Settings>
        <Networking>
            <UDPPort>1776</UDPPort>
            <ServerURL></ServerURL>
            <PacketSize></PacketSize>
            <PacketSendInterval>1</PacketSendInterval>
            <ClientHeartbeatSendInterval>1</ClientHeartbeatSendInterval>
            <MaxTimeBeforeDisconnect>10</MaxTimeBeforeDisconnect>
            <CheckInactiveConnectionsInterval>10</CheckInactiveConnectionsInterval>
            <HeartbeatMessage>.</HeartbeatMessage>
            <SendDataInterval>1</SendDataInterval>
            <ClientDebugHost>127.0.0.2</ClientDebugHost>
            <ServiceDebugHost>127.0.0.1</ServiceDebugHost>
        </Networking>
        <Security>
            <!--Possible modes are "Disabled", "Insecure" (passwords), and "Secure" (RSA key).
                Note that changing this will only work if the server is configured to the same mode as specified.-->
            <AuthenticationMode>Secure</AuthenticationMode>
        </Security>
    </Settings>
</Parameters>